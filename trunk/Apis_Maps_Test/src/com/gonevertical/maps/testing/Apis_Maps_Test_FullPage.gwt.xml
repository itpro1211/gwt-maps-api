<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='apis_maps_test_fullpage'>


  <!-- Inherit the core Web Toolkit stuff.                        -->
  <inherits name='com.google.gwt.user.User'/>
  
  
  <!-- Inherit the default GWT style sheet.  				      -->
  
  
  <!-- I'm using json for debugging -->
  <inherits name="com.google.gwt.json.JSON" />
  

  <!-- Other module inherits                                      -->
  <inherits name='com.google.gwt.maps.Apis_Google_Maps'/>


  <!-- Specify the app entry point class.                         -->
  <entry-point class='com.gonevertical.maps.testing.client.Apis_Maps_Test_FullPage'/>


  <!-- Style sheets -->
  <stylesheet src="css/style.css"/>


  <!-- Specify the paths for translatable code                    -->
  <source path='client'/>
  

</module>
